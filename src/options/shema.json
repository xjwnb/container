            "borderRadius": {
              "type": "string",
              "description": "样式 border-radius 属性，默认为 10px"
            },
            "boxSizing": {
              "type": "string",
              "description": "样式 box-sizing 属性，默认为 border-box"
            },
            "display": {
              "type": "string",
              "description": "样式 display 属性，默认为 grid"
            },
            "gridTemplateColumns": {
              "type": "string",
              "description": "样式 grid-template-columns 属性，默认为 repeat(auto-fill)"
