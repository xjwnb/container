        "style": {
          "type": "object",
          "description": "根容器样式",
          "properties": {
            "width": {
              "type": "string",
              "description": "样式 width 属性，默认为 100%"
            },
            "padding": {
              "type": "string",
              "description": "样式 padding 属性，默认为 10px"
            },
            "background": {
              "type": "string",
              "description": "样式 background 属性，默认为 rgba(0, 0, 0, .6)"
            },
            "border": {
              "type": "string",
              "description": "样式 border 属性，默认为 ''"
